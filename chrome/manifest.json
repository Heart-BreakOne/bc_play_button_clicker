{
  "manifest_version": 3,
  "name": "Bracket City Play button Clicker",
  "version": "1.1",
  "description": "Automatically clicks the Play button on Bracket City",
  "content_scripts": [
    {
      "matches": ["https://www.theatlantic.com/games/bracket-city/*"],
      "js": ["script.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
